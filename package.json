{
  "name": "unofficial-amazon-search",
  "version": "1.0.0",
  "description": "A simple client for searching Amazon",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -f dist/*.js* && rm -f dist/*.ts*",
    "compile": "tsc && webpack --config webpack.config.js",
    "build": "npm run clean && npm run compile",
    "build-dev": "npm run clean && npm run compile-dev",
    "compile-dev": "tsc && webpack.dev.config.js"
  },
  "author": "Kyle Nazario",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kyle-n/unofficial-amazon-search.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/kyle-n/unofficial-amazon-search/issues"
  },
  "homepage": "https://github.com/kyle-n/unofficial-amazon-search#readme",
  "dependencies": {
    "isomorphic-fetch": "^2.2.1",
    "jsdom": "^16.2.2",
    "jsonp-client": "^1.2.2"
  },
  "type": "commonjs",
  "devDependencies": {
    "@types/isomorphic-fetch": "0.0.35",
    "@types/jsdom": "^16.2.1",
    "@types/node": "^13.13.5",
    "path": "^0.12.7",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}
